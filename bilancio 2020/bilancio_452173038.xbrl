&lt;td valign="middle" style="width:222.7pt; background-color:#CADFF0; padding-top:3.5pt; padding-right:3pt; padding-bottom:1.5pt; padding-left:3pt; border-top: 1pt solid #91C5D3; border-right: 1pt solid #91C5D3; border-bottom: 1pt solid #91C5D3; border-left: 1pt solid #91C5D3;"&gt;
&lt;p style="text-align:center;margin-top:0pt;margin-bottom:0pt;line-height:10pt;"&gt;&lt;span style="font-size:8.5pt;color:#000000;font-weight:bold;"&gt; &lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td valign="middle" style="width:114.7pt; background-color:#CADFF0; padding-top:3.5pt; padding-right:3pt; padding-bottom:1.5pt; padding-left:3pt; border-top: 1pt solid #91C5D3; border-right: 1pt solid #91C5D3; border-bottom: 1pt solid #91C5D3; border-left: 1pt solid #91C5D3;"&gt;
&lt;p style="text-align:center;margin-top:0pt;margin-bottom:0pt;line-height:10pt;"&gt;&lt;span style="font-size:8.5pt;color:#000000;font-weight:bold;"&gt;Bilancio 31/12/2019&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td valign="middle" style="width:111.75pt; background-color:#CADFF0; padding-top:3.5pt; padding-right:3pt; padding-bottom:1.5pt; padding-left:3pt; border-top: 1pt solid #91C5D3; border-right: 1pt solid #91C5D3; border-bottom: 1pt solid #91C5D3; border-left: 1pt solid #91C5D3;"&gt;
&lt;p style="text-align:center;margin-top:0pt;margin-bottom:0pt;line-height:10pt;"&gt;&lt;span style="font-size:8.5pt;color:#000000;font-weight:bold;"&gt;Bilancio 31/12/2018&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
